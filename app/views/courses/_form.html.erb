<%= form_for(@course, html:{class: "ui large form", role: "form"}) do |f|%>
  <div class="two fields">
    <div class="field">
      <label>Course Name</label>
     <%= f.text_field :name%>
    </div>
    <div class="field">
      <label>Course decription</label>
      <%= f.text_field :description%>
    </div>
  </div>
  <div ><%= f.submit(class: "ui submit button") %></div>
<% end %>
    
    